<setup>
  <block title="Configuration of Catalyst Cloud App for Splunk">
    <text>Enter your account details of Catalyst Cloud</text>
  </block>
  <block title="Credentials of your Catalyst Cloud Account" endpoint="storage/passwords" entity="_new">
      <input field="name">
        <label>Username</label>
        <type>text</type>
      </input>
      <input field="password">
        <label>Password</label>
        <type>password</type>
      </input>
  </block>
  <block title="" endpoint="mycustom/customendpoint" entity="userinfo">
      <input field="tenant">
        <label>Project Name</label>
        <type>text</type>
      </input>
      <input field="baseurl">
        <label>Auth URL</label>
        <type>text</type>
      </input>
  </block>
</setup>
